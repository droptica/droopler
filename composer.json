{
    "name": "droptica/droopler",
    "description": "Droopler installation profile for Drupal 8",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "bower-asset/colorbox": "^1.6",
        "bower-asset/lazysizes": "^5.2",
        "bower-asset/masonry": "^4.2",
        "drupal/admin_toolbar": "^2.0",
        "drupal/advagg": "^4.0",
        "drupal/better_exposed_filters": "^4.0",
        "drupal/checklistapi": "^1.8",
        "drupal/colorbox": "^1.4",
        "drupal/config_update": "^1.3",
        "drupal/contact_formatter": "^2.0",
        "drupal/core": "~8.0",
        "drupal/entity_reference_display": "^1.2",
        "drupal/entity_reference_revisions": "^1.3",
        "drupal/facets": "^1.0",
        "drupal/features": "^3.5",
        "drupal/field_group": "^3.0",
        "drupal/geysir": "^1.2",
        "drupal/google_analytics": "^2.2",
        "drupal/google_tag": "1.x-dev",
        "drupal/lazy": "^3.4",
        "drupal/link_attributes": "^1.1",
        "drupal/linkit": "^4.3",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.3",
        "drupal/paragraphs": "^1.1",
        "drupal/pathauto": "^1.0",
        "drupal/redirect": "^1.0-beta1",
        "drupal/search_api": "^1.8",
        "drupal/simple_sitemap": "^3.3",
        "drupal/smtp": "^1.0@beta",
        "drupal/svg_image": "^1.8",
        "drupal/tvi": "^1.0-beta2",
        "drupal/we_megamenu": "^1.5",
        "drupal/schema_metatag": "^1.4",
        "npm-asset/countup": "^1.9",
        "npm-asset/in-viewport": "^3.6",
        "npm-asset/select2": "^4.0.4",
        "npm-asset/slick-carousel": "^1.8",
        "npm-asset/object-fit-images": "^3.2.4"
    },
    "replace": {
        "bower-asset/desandro-matches-selector": "*",
        "bower-asset/ev-emitter": "*",
        "bower-asset/fizzy-ui-utils": "*",
        "bower-asset/get-size": "*",
        "bower-asset/jquery": "*",
        "bower-asset/outlayer": "*"
