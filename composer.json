{
    "name": "droptica/droopler",
    "description": "Droopler installation profile for Drupal 8",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "npm-asset/bootstrap": "4.0.0",
        "npm-asset/countup": "^1.9",
        "npm-asset/in-viewport": "^3.6",
        "npm-asset/jquery-backstretch": "^2.1",
        "npm-asset/popper.js": "1.12.3",
        "npm-asset/slick-carousel": "^1.8",
        "bower-asset/colorbox": "^1.6",
        "bower-asset/masonry": "^4.2",
        "bower-asset/lato-webfont": "^1.1",
        "drupal/admin_toolbar": "^1.19",
        "drupal/colorbox": "^1.4",
        "drupal/config_update": "^1.3",
        "drupal/contact_formatter": "^1.0",
        "drupal/core": "~8.0",
        "drupal/entity_reference_revisions": "^1.3",
        "drupal/features": "^3.5",
        "drupal/field_group": "^3.0",
        "drupal/google_analytics": "^2.2",
        "drupal/google_tag": "1.x-dev",
        "drupal/link_attributes": "^1.1",
        "drupal/linkit": "^4.3",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.3",
        "drupal/paragraphs": "^1.1",
        "drupal/pathauto": "^1.0",
        "drupal/redirect": "^1.0-beta1",
        "drupal/simple_sitemap": "^2.11",
        "drupal/smtp": "^1.0@beta",
        "drupal/svg_image": "^1.8",
        "drupal/tvi": "^1.0-beta2"

    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true
    }
}
