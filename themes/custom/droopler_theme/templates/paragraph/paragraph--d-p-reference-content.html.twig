{% embed '@droopler_theme/templates/paragraph/paragraph.html.twig' %}

  {% block component_content %}
    {% embed '@droopler_theme/d-p-reference-content/d-p-reference-content.twig' %}
      {% block header %}
        {{ content.group_header }}
      {% endblock %}

      {% block content %}
        {{ content.group_content }}
      {% endblock %}

      {% block items %}
        {{ content.group_items }}
      {% endblock %}

      {% block cta %}
        {{ content.group_cta }}
      {% endblock %}
    {% endembed %}
  {% endblock %}

  {% block content %}
    {{ content|without('group_header', 'group_content', 'group_items', 'group_cta') }}
  {% endblock %}

{% endembed %}
